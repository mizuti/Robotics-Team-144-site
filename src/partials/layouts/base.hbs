<body id="page-top">
    {{> navbar}}
    <main>
        {{#> content }}
            <h2>This is default content</h2>
        {{/content}}
    </main>
<footer class="bg-light" id="footer">
    <div class="container">
        <div class="row">
            <div class="col-md-4 text-center">
                <h3 class="section-heading">Want to get involved?</h3>
                <p>Send us and email asking to recieve our team updates.</p>
                <i class="fa fa-envelope-o fa-3x sr-contact"></i>
                <p><a href="mailto:your-email@your-domain.com">team144@email.com</a></p>
            </div>
            <div class="col-xs-12 col-md-4 text-center">
                <div class="btn btn-primary btn-center">Get Sponsorship Info</div>
            </div>
            <div class="col-xs-12 col-md-4 text-center pull-right">
                <h2 class="col-xs-12">Sponsors</h2>
                {{#each sponsors}}
                    <h3 class="">{{title}}</h3>
                {{/each}}
            </div>
        </div>
    </div>
</footer>

